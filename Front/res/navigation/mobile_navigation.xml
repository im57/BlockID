<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_main">


    <fragment
        android:id="@+id/nav_main"
        android:name="hanium.mobile.did_student.ui.main.MainFragment"
        android:label="@string/menu_main"
        tools:layout="@layout/fragment_main" />

    <fragment
        android:id="@+id/nav_mypage_edit"
        android:name="hanium.mobile.did_student.ui.mypage.EditInfoFragment"
        android:label="@string/menu_mypage_edit"
        tools:layout="@layout/fragment_mypage_edit" />

    <fragment
        android:id="@+id/nav_attendance_check"
        android:name="hanium.mobile.did_student.ui.attendance.CheckAttendanceFragment"
